    "def StringReverse(str1):\n",
    "    str2 = str1[::-1]\n",
    "    return str2\n",
    "string = input(\"Please enter a String : \")\n",
    "string2 = StringReverse(string)\n",
    "print(\"The Reversed String = \", string2)"
